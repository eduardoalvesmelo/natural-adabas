/** Fazer um programa para mostrar os 15 primeiros termos da sÃ©rie de Fibonacci. 
/**
/** :author eduar
/* TODO Enter your code here
DEFINE DATA LOCAL
1 #i                (N4) init<0> 
1 #j                (N4) init<0>
1 #qtde-termos      (N4)
1 #seq-fibonacci    (N10/*)

END-DEFINE       
*
    input 'Informe a quantidade de termos a serem exibidos' #qtde-termos    
*    
*    #seq-fibonacci(1):= 1
     for #i:= 1 to #qtde-termos
         #j:= #i - 1
        #seq-fibonacci(#i):= #seq-fibonacci(#i) + #seq-fibonacci(#j)
     end-for
*
    for #i:= 1 to #qtde-termos
        write #seq-fibonacci(#i)
     end-for
 
END

