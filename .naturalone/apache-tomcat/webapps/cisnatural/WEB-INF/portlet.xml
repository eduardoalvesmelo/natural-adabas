<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd" 
             version="1.0" 	
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
             xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd 	http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
             id="PortletWrapper">
    <portlet>
        <description>PortletWrapper</description>
        <portlet-name>PortletWrapper</portlet-name>
        <display-name>PortletWrapper</display-name>
        <portlet-class>com.softwareag.cis.server.PortletWrapper</portlet-class>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
        </supports>
        <portlet-info>
            <title>PortletWrapper</title>
            <short-title>PortletWrapper</short-title>
        </portlet-info>
    </portlet>
</portlet-app> 
